{
    "name": "appshell",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "bundle-android": "react-native bundle --entry-file index.js --bundle-output ./bundle/android/bkddBundle/index.bundle --platform android --assets-dest ./bundle/android/bkddBundle --dev false",
        "bundle-ios": "react-native bundle --entry-file index.js --bundle-output ./bundle/ios/bkddBundle/index.bundle --platform ios --assets-dest ./bundle/ios/bkddBundle --dev false"
    },
    "dependencies": {
        "react": "16.6.3",
        "react-native": "0.57.8",
        "react-native-device-info": "^0.26.4",
        "react-native-fs": "^2.13.3",
        "react-native-network-info": "^4.0.1",
        "react-native-permissions": "^1.1.1",
        "react-native-root-siblings": "^3.1.7",
        "react-native-root-toast": "^3.0.2",
        "react-native-splash-screen": "^3.2.0",
        "react-native-zip-archive": "^3.0.1"
    },
    "devDependencies": {
        "babel-jest": "23.6.0",
        "metro-react-native-babel-preset": "0.51.1",
        "react-test-renderer": "16.6.3"
    },
    "jest": {
        "preset": "react-native"
    }
}
